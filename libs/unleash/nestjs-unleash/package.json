{
  "name": "nestjs-unleash",
  "version": "2.2.0",
  "description": "Unleash integration for NestJS",
  "author": "Philipp Busse",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bugs": {
    "url": "https://github.com/pmb0/nestjs-unleash/issues"
  },
  "homepage": "https://github.com/bussedev/monorepo/tree/main/libs/unleash/nestjs-unleash#readme",
  "repository": {
    "url": "git@github.com:bussedev/monorepo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start:dev": "nodemon  --exec ts-node --files e2e/src/main.ts",
    "lint": "eslint --cache ./src --ignore-path ../../../.eslintignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "engines": {
    "node": ">=14.17.0",
    "pnpm": ">=6.19.0"
  },
  "dependencies": {
    "ip": "^1.1.5",
    "murmurhash3js": "^3.0.1",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/schedule": "^2.1.0",
    "axios": "^0.27.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@busse/jest-config": "workspace:*",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@heise/eslint-config": "^20.0.33",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.0.0",
    "@types/murmurhash3js": "^3.0.2",
    "@types/node": "^16.11.56",
    "@types/supertest": "^2.0.12",
    "@unleash/client-specification": "^4.0.0",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "mockdate": "^3.0.2",
    "nodemon": "^2.0.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "files": [
    "dist"
  ]
}
