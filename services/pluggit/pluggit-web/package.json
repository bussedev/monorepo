{
  "bin": "bin/main.js",
  "name": "@busse/nest-pluggit-web",
  "version": "1.0.12",
  "license": "UNLICENSED",
  "main": "dist",
  "pkg": {
    "targets": [
      "node16-alpine"
    ],
    "outputPath": ".build"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "start:dev": "tsc-watch  --build tsconfig.build.json --onSuccess 'node --inspect=0.0.0.0:9229 --enable-source-maps dist/main' --preserveWatchOutput | pino-pretty",
    "test": "jest --passWithNoTests",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint --cache ./src --report-unused-disable-directives --ignore-path ../../../.eslintignore",
    "build:pkg": "pkg --options 'max-old-space-size=350' --compress GZip  --no-bytecode --public-packages '*' --public -o .build/main .",
    "docker:build": "docker build -t ghcr.io/bussedev/pluggit-collector -f ../../../Dockerfile .",
    "docker:publish": "../../../docker-publish.sh"
  },
  "dependencies": {
    "@busse/nest-pluggit": "workspace:*",
    "@busse/pluggit": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "nestjs-pino": "^3.1.1",
    "pino-http": "^8.2.0",
    "pino-pretty": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@busse/jest-config": "workspace:*",
    "@jest/types": "^28.1.3",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/testing": "^8.4.7",
    "@types/node": "^16.11.56",
    "jest": "^29.0.2",
    "jest-junit": "^13.2.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14.17.0",
    "pnpm": ">=6.19.0"
  },
  "private": true,
  "peerDependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/terminus": "^8.1.1"
  },
  "bugs": {
    "url": "https://github.com/bussedev/monorepo/issues"
  },
  "homepage": "https://github.com/bussedev/monorepo/tree/main/services/pluggit/pluggit-web#readme",
  "repository": {
    "url": "git@github.com:bussedev/monorepo.git"
  }
}
